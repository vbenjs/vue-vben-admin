    "path-to-regexp": "^6.2.0",
    "pinia": "2.0.0-alpha.13",
    "print-js": "^1.6.0",
    "qrcode": "^1.4.4",
    "sortablejs": "^1.13.0",
    "tinymce": "^5.7.1",
